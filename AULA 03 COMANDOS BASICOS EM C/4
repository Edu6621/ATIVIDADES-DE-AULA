/*Escreva um programa que receba dois números reais, faça uma divisão entre eles e mostre na tela o divisor, 
o dividendo, o quociente e o resto da divisão */
#include <stdio.h>
int main () {
    int divisor, dividendo, quociente, resto;
    printf ("insira um número : ");
    scanf ("%i", &dividendo);
    printf ("insira um número para dividir : ");
    scanf ("%i", &divisor);
    quociente=dividendo/divisor;
    resto=dividendo%divisor;
    printf ("Divisor : %i\nDividendo : %i\nQuociente : %i\nResto da Divisão : %i\n",divisor,dividendo,quociente,resto);
    return 0;   
}
