/*Faça um programa que leia a velocidade de um veículo em Km/H e calcule e imprima a velocidade em 
m/s. */
#include <stdio.h>
int main () {
    int km, ms;
    printf ("informe a velocidade em Km/h : ");
    scanf ("%i", &km);
    ms=km/1.6;
    printf ("a velocidade em m/s é : %i", ms);
    return 0;
}
